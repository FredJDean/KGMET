    "mom2_dtype": "float32",
    "nullspace_threshold":2e-2,
    "L2":10,
    "subgraph_size": 35,
    "gnn_fact_token_strategy": "avg",
    "gnn_weight_decay": 1e-1,
    "gnn_dim_factor": 1,
    "gnn_attn_drop": 0.2,
    "gnn_feat_drop": 0.15,
    "get_repr_layer": 5,
