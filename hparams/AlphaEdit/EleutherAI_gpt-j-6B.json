{   
    "model_name": "EleutherAI_gpt-j-6B",
    "layers": [
        3, 4, 5, 6, 7, 8
    ],
    "clamp_norm_factor": 0.75,
    "layer_selection": "all",
    "fact_token": "subject_last",
    "v_num_grad_steps": 30,
    "v_lr": 5e-1,
    "v_loss_layer": 27,
    "v_weight_decay": 0.5,
    "kl_factor": 0.0625,
